# LEAN Trading Bot Stack - Konfiguracja środowiska
# Skopiuj ten plik do .env i wypełnij własnymi wartościami
# NIGDY nie commituj pliku .env do repozytorium!

# ===============================================
# PODSTAWOWA KONFIGURACJA
# ===============================================

# Środowisko uruchomieniowe (development/production)
ENVIRONMENT=development

# Hasla i klucze bezpieczeństwa (generowane automatycznie przez install.sh)
POSTGRES_PASSWORD=change_me_postgres_password
REDIS_PASSWORD=change_me_redis_password
FLASK_SECRET_KEY=change_me_flask_secret_key

# ===============================================
# QUANTCONNECT LEAN KONFIGURACJA
# ===============================================

# API QuantConnect (opcjonalne - dla cloudowego backtestingu)
QC_API_ACCESS_TOKEN=your_quantconnect_api_token
QC_USER_ID=your_quantconnect_user_id

# ===============================================
# TUNELOWANIE I DOMENA
# ===============================================

# Typ tunelowania: none, ngrok, localtunnel, serveo, cloudflare, pagekite, telebit, domain
TUNNEL_TYPE=none

# Konfiguracja Ngrok
NGROK_AUTH_TOKEN=your_ngrok_auth_token
NGROK_REGION=us  # us, eu, ap, au, sa, jp, in

# Konfiguracja LocalTunnel
LOCALTUNNEL_SUBDOMAIN=your_subdomain

# Konfiguracja Serveo
SERVEO_SUBDOMAIN=your_subdomain

# Konfiguracja Cloudflare Tunnel
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token

# Konfiguracja PageKite
PAGEKITE_KITE=your_kite_name.pagekite.me
PAGEKITE_SECRET=your_pagekite_secret

# Konfiguracja Telebit
TELEBIT_TOKEN=your_telebit_token

# Własna domena
DOMAIN_NAME=yourdomain.com

# ===============================================
# BROKERZY - FX/CFD
# ===============================================

# XM
XM_API_KEY=your_xm_api_key
XM_API_SECRET=your_xm_api_secret
XM_ACCOUNT_ID=your_xm_account_id
XM_ENVIRONMENT=demo  # demo lub live

# IC Markets
IC_MARKETS_API_KEY=your_ic_markets_api_key
IC_MARKETS_API_SECRET=your_ic_markets_api_secret
IC_MARKETS_ACCOUNT_ID=your_ic_markets_account_id
IC_MARKETS_ENVIRONMENT=demo  # demo lub live

# RoboForex
ROBOFOREX_API_KEY=your_roboforex_api_key
ROBOFOREX_API_SECRET=your_roboforex_api_secret
ROBOFOREX_ACCOUNT_ID=your_roboforex_account_id
ROBOFOREX_ENVIRONMENT=demo

# InstaForex
INSTAFOREX_API_KEY=your_instaforex_api_key
INSTAFOREX_API_SECRET=your_instaforex_api_secret
INSTAFOREX_ACCOUNT_ID=your_instaforex_account_id

# FBS
FBS_API_KEY=your_fbs_api_key
FBS_API_SECRET=your_fbs_api_secret
FBS_ACCOUNT_ID=your_fbs_account_id

# XTB
XTB_USER_ID=your_xtb_user_id
XTB_PASSWORD=your_xtb_password
XTB_ENVIRONMENT=demo  # demo lub real

# Admiral Markets
ADMIRAL_API_KEY=your_admiral_api_key
ADMIRAL_API_SECRET=your_admiral_api_secret
ADMIRAL_ACCOUNT_ID=your_admiral_account_id

# IG Group
IG_API_KEY=your_ig_api_key
IG_USERNAME=your_ig_username
IG_PASSWORD=your_ig_password
IG_ENVIRONMENT=demo  # demo lub live

# Plus500
PLUS500_API_KEY=your_plus500_api_key
PLUS500_API_SECRET=your_plus500_api_secret

# SabioTrade
SABIOTRADE_API_KEY=your_sabiotrade_api_key
SABIOTRADE_API_SECRET=your_sabiotrade_api_secret
SABIOTRADE_ACCOUNT_ID=your_sabiotrade_account_id

# ===============================================
# BROKERZY - KRYPTO/EXCHANGE
# ===============================================

# Binance
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret
BINANCE_TESTNET=true  # true for testnet, false for mainnet
BINANCE_US=false      # true for Binance US

# Coinbase Pro
COINBASE_API_KEY=your_coinbase_api_key
COINBASE_API_SECRET=your_coinbase_api_secret
COINBASE_PASSPHRASE=your_coinbase_passphrase
COINBASE_SANDBOX=true  # true for sandbox, false for live

# Kraken
KRAKEN_API_KEY=your_kraken_api_key
KRAKEN_API_SECRET=your_kraken_api_secret

# Bitstamp
BITSTAMP_API_KEY=your_bitstamp_api_key
BITSTAMP_API_SECRET=your_bitstamp_api_secret
BITSTAMP_CUSTOMER_ID=your_bitstamp_customer_id

# Bitfinex
BITFINEX_API_KEY=your_bitfinex_api_key
BITFINEX_API_SECRET=your_bitfinex_api_secret

# Gemini
GEMINI_API_KEY=your_gemini_api_key
GEMINI_API_SECRET=your_gemini_api_secret
GEMINI_SANDBOX=true   # true for sandbox, false for live

# Huobi
HUOBI_API_KEY=your_huobi_api_key
HUOBI_API_SECRET=your_huobi_api_secret

# OKX
OKX_API_KEY=your_okx_api_key
OKX_API_SECRET=your_okx_api_secret
OKX_PASSPHRASE=your_okx_passphrase
OKX_SANDBOX=true      # true for sandbox, false for live

# Bybit
BYBIT_API_KEY=your_bybit_api_key
BYBIT_API_SECRET=your_bybit_api_secret
BYBIT_TESTNET=true    # true for testnet, false for mainnet

# KuCoin
KUCOIN_API_KEY=your_kucoin_api_key
KUCOIN_API_SECRET=your_kucoin_api_secret
KUCOIN_PASSPHRASE=your_kucoin_passphrase
KUCOIN_SANDBOX=true   # true for sandbox, false for live

# Bittrex
BITTREX_API_KEY=your_bittrex_api_key
BITTREX_API_SECRET=your_bittrex_api_secret

# ===============================================
# MT4/MT5 BRIDGE KONFIGURACJA
# ===============================================

# Ogólne ustawienia MT4/MT5 Bridge
MT_BRIDGE_ENABLED=false
MT_BRIDGE_TYPE=mt4     # mt4 lub mt5
MT_BRIDGE_HOST=localhost
MT_BRIDGE_PORT=8222

# Konfiguracja dla różnych brokerów MT4/MT5
# XM MT4
XM_MT4_SERVER=XMGlobal-Demo
XM_MT4_LOGIN=your_mt4_login
XM_MT4_PASSWORD=your_mt4_password

# IC Markets MT4
IC_MT4_SERVER=ICMarkets-Demo01
IC_MT4_LOGIN=your_ic_mt4_login
IC_MT4_PASSWORD=your_ic_mt4_password

# RoboForex MT4
ROBOFOREX_MT4_SERVER=RoboForex-Demo
ROBOFOREX_MT4_LOGIN=your_roboforex_mt4_login
ROBOFOREX_MT4_PASSWORD=your_roboforex_mt4_password

# ===============================================
# ML/AI KONFIGURACJA
# ===============================================

# Ścieżki do modeli
MODEL_STORAGE_PATH=./models
ONNX_RUNTIME_PROVIDER=CPUExecutionProvider  # CPUExecutionProvider, CUDAExecutionProvider

# TensorFlow ustawienia
TF_CPP_MIN_LOG_LEVEL=2
TF_FORCE_GPU_ALLOW_GROWTH=true

# Jupyter Lab konfiguracja
JUPYTER_ENABLE_LAB=yes
JUPYTER_TOKEN=your_jupyter_token_or_empty_for_no_auth

# ===============================================
# BEZPIECZEŃSTWO I MONITORING
# ===============================================

# Rate limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Logging
LOG_LEVEL=INFO          # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_TO_FILE=true
LOG_MAX_SIZE_MB=100
LOG_BACKUP_COUNT=5

# SSL/TLS (dla produkcji)
SSL_ENABLED=false
SSL_CERT_PATH=/etc/ssl/certs/trading-bot.crt
SSL_KEY_PATH=/etc/ssl/private/trading-bot.key

# Firewall i IP whitelist (opcjonalne)
FIREWALL_ENABLED=false
ALLOWED_IPS=127.0.0.1,::1  # Rozdzielone przecinkami

# Backup konfiguracja
BACKUP_ENABLED=false
BACKUP_SCHEDULE=0 2 * * *  # Cron expression - codziennie o 2 rano
BACKUP_RETENTION_DAYS=30

# ===============================================
# ZAAWANSOWANE USTAWIENIA
# ===============================================

# WebUI konfiguracja
WEBUI_PORT=3000
API_PORT=5000
API_WORKERS=4

# Database connection pooling
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=30
DB_POOL_TIMEOUT=30

# Redis konfiguracja
REDIS_DB=0
REDIS_MAX_CONNECTIONS=50

# Timeout ustawienia
API_TIMEOUT_SECONDS=30
BROKER_CONNECTION_TIMEOUT=10
BROKER_REQUEST_TIMEOUT=30

# ===============================================
# DEVELOPMENT/DEBUG USTAWIENIA
# ===============================================

# Debug mode (tylko dla development!)
DEBUG_MODE=false
FLASK_DEBUG=false

# Profiling
PROFILING_ENABLED=false

# Test mode
TEST_MODE=false
PAPER_TRADING_ONLY=true    # WAŻNE: Ustaw na false tylko gdy jesteś gotowy na live trading!